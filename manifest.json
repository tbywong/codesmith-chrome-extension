{
	"manifest_version": 2,

	"name": "Now Playing",
	"description": "This extension shows a Google Image search result for the current page",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/",
		"https://api.themoviedb.org/3/movie/550?api_key=0fdbb8fb91fc7d7557228fd3d1fcaa52"
	]
}
